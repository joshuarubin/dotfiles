{
  "coc.preferences.formatOnType": true,
  "coc.preferences.hoverTarget": "float",
  "coc.preferences.formatOnSaveFiletypes": ["typescript", "rust", "python"],
  "coc.preferences.jumpCommand": "edit",
  "coc.preferences.useQuickfixForLocations": false,
  "codeLens.enable": true,
  "diagnostic.messageTarget": "echo",
  "diagnostic.displayByAle": true,
  "diagnostic.refreshOnInsertMode": true,
  "diagnostic.errorSign": "üî•",
  "diagnostic.warningSign": "üöß",
  "diagnostic.infoSign": "üí¨",
  "diagnostic.hintSign": "üí¨",
  "git.addGBlameToBufferVar": true,
  "git.addedSign.hlGroup": "GitGutterAdd",
  "git.addedSign.text": "‚ñé",
  "git.changeRemovedSign.hlGroup": "GitGutterChangeDelete",
  "git.changeRemovedSign.text": "‚ñã",
  "git.changedSign.hlGroup": "GitGutterChange",
  "git.changedSign.text": "‚ñé",
  "git.removedSign.hlGroup": "GitGutterDelete",
  "git.removedSign.text": "‚ñè",
  "git.topRemovedSign.hlGroup": "GitGutterDelete",
  "git.topRemovedSign.text": "‚ñî",
  "javascript.preferences.noSemicolons": true,
  "python.linting.pylintUseMinimalCheckers": false,
  "python.linting.flake8Enabled": true,
  "python.formatting.provider": "black",
  "rust-analyzer.enableCargoWatchOnStartup": true,
  "signature.preferShownAbove": false,
  "signature.target": "float",
  "suggest.detailField": "abbr",
  "suggest.enablePreselect": false,
  "suggest.floatEnable": true,
  "suggest.noselect": true,
  "suggest.snippetIndicator": "üåü",
  "suggest.triggerAfterInsertEnter": true,
  "tslint.autoFixOnSave": true,
  "typescript.preferences.noSemicolons": true,
  "rust.crate_blacklist": {
    "default": true,
    "description": "Don't index crates on the crate blacklist.",
    "scope": "resource",
    "type": "boolean"
  },
  "languageserver": {
    "golang": {
      "args": ["serve"],
      "command": "gopls",
      "filetypes": ["go"],
      "rootPatterns": ["go.mod", ".vim/", ".git/", ".hg/"],
      "settings": {
        "gopls": {
          "usePlaceholders": true,
          "codelens": { "test": true }
        }
      }
    }
  }
}
